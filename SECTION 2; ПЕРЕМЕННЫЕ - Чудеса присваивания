//Создайте переменную с именем a. Присвойте переменной значение 15.
let a = 15;
console.log(a);
//Выведите значение переменной a в консоль.

//Напишите команду, увеличивающую значение переменной а на 1. Выведите результат в консоль.
a = a + 1;
console.log(a);

//Напишите команду, уменьшающую значение переменной а на 2. Выведите результат в консоль.
a = a - 2;
console.log(a);

//Напишите команду, увеличивающую значение переменной а в три раза. Выведите результат в консоль.
a = a + 3;
console.log(a);

//Напишите команду, уменьшающую значение переменной а в два раза. Выведите результат в консоль.
a = a - 2;
console.log(a);

//Напишите команду, возводящую значение переменной а в квадрат. Выведите результат в консоль.
a = a * a;
console.log(a);

//Создайте переменную с именем s. Присвойте переменной s значение 125. 
let s = 125;

//Напишите последовательность команд, в результате которых переменная s 
//будет принимать значения 25, 5, 0.
s = s / 5;
console.log(s);
s = s / 5;
console.log(s);
s = s - s;
console.log(s);
//Создайте переменную именем х. Присвойте переменной х значение 12.
let x = 12;

//Создайте переменную именем у. Присвойте переменной у значение 21.
let y = 21;

//Напишите последовательность команд, в результате которых 
//переменные х и у обменяются своими значениями. Выведите значения переменных х и у в консоль.
let temp;
temp = x; // temp = 12;
x = y; // x = 21;
y = temp; // y = 12;
console.log(x,y);














